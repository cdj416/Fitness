<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/memberCardBox"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_FF333333"
                android:drawableTop="@mipmap/member_img"
                android:gravity="center"
                android:drawablePadding="3dp"
                android:text="健身购卡"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/encyclopediaBox"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_FF333333"
                android:drawableTop="@mipmap/encyclopedia_img"
                android:gravity="center"
                android:drawablePadding="3dp"
                android:text="健康百科"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/pointsCenterBox"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_FF333333"
                android:drawableTop="@mipmap/points_center_img"
                android:gravity="center"
                android:drawablePadding="3dp"
                android:text="积分中心"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/goVue"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_FF333333"
                android:drawableTop="@mipmap/vue_img"
                android:gravity="center"
                android:drawablePadding="3dp"
                android:text="运动场馆"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/outdoorBox"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/color_FF333333"
                android:drawableTop="@mipmap/outdoor"
                android:gravity="center"
                android:drawablePadding="3dp"
                android:text="户外运动"/>
        </LinearLayout>
